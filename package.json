{
  "name": "@dljsbook/data",
  "version": "0.9.4",
  "description": "This is a supporting library for the book Deep Learning With Javascript (https://dljsbook.com).",
  "author": "Kevin Scott <kevin@dljsbook.com>",
  "license": "MIT",
  "repository": "https://github.com/dljsbook/data",
  "module": "dist/index.js",
  "main": "dist/index.js",
  "browser": "dist/dljsData.umd.min.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build:typescript": "tsc -p tsconfig.json",
    "build:webpack": "webpack --config webpack.config.js",
    "build": "yarn clean && yarn lint && yarn build:typescript && yarn build:webpack",
    "clean": "rimraf dist",
    "develop": "tsc -p tsconfig.json --watch",
    "lint": "tslint -p . -t verbose",
    "prepublish": "yarn build",
    "test": "jest --watchAll"
  },
  "dependencies": {
    "@types/color": "^3.0.0",
    "chart.js": "^2.7.3",
    "color": "^3.1.0"
  },
  "peerDependencies": {
    "@tensorflow/tfjs": "^0.14.0"
  },
  "devDependencies": {
    "@babel/core": "^7.1.6",
    "@tensorflow/tfjs": "^0.14.2",
    "@types/chart.js": "^2.7.43",
    "@types/jest": "^23.3.10",
    "@types/node": "^10.12.18",
    "gh-pages": "^2.0.1",
    "jest": "^24.0.0",
    "rimraf": "^2.6.2",
    "ts-jest": "^23.10.5",
    "tslib": "^1.9.3",
    "typescript": "^3.1.6",
    "webpack": "^4.29.0",
    "webpack-cli": "^3.2.1"
  },
  "jest": {
    "roots": [
      "<rootDir>/src"
    ],
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.tsx?$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
